ARCH="x86"
ABI="x86"
ACCEPT_KEYWORDS="x86"
MULTILIB_ABIS="x86"
DEFAULT_ABI="x86"
IUSE_IMPLICIT="abi_x86_32"
LIBDIR_x86="lib"

CHOST="i686-pc-linux-gnu"
CFLAGS="-O2 -mtune=i686 -pipe"
CXXFLAGS="${CFLAGS}"

# kvm defaults
QEMU_SOFTMMU_TARGETS="i386"
QEMU_USER_TARGETS="i386"

GRUB_PLATFORMS="efi-32 pc"

# FL-2055: CPU instruction set flags
USE_EXPAND_HIDDEN="-CPU_FLAGS_X86"
